  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "1.13.2",
    "webpack-cli": "^3.1.0"
  }
